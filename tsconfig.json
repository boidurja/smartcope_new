{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "target": "es2015",
    "lib": ["es7"],
	"skipLibCheck": true,
	"baseUrl": "./app",
	"paths": {
		"screens": ["screens"],
		"components": ["components"],
		"models": ["models"],
		"services": ["services"],
		"theme": ["theme"],
		"navigation": ["navigation"],
		"utils": ["utils"],
		"screens/*": ["screens/*"],
		"components/*": ["components/*"],
		"models/*": ["models/*"],
		"services/*": ["services/*"],
		"theme/*": ["theme/*"],
		"navigation/*": ["navigation/*"],
		"utils/*": ["utils/*"],
	}
  },
  "exclude": ["node_modules"],
  "include": ["index.js", "app", "test", "storybook"]
}
